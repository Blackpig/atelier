{
    "name": "blackpig-creatif/atelier",
    "description": "L'art du contenu - Artisanal content blocks for FilamentPHP v4",
    "keywords": [
      "laravel",
      "filament",
      "filamentphp",
      "cms",
      "blocks",
      "content-management",
      "page-builder",
      "blackpig-creatif",
      "atelier"
    ],
    "homepage": "https://github.com/blackpig-creatif/atelier",
    "license": "MIT",
    "authors": [
      {
        "name": "Stuart Hallewell",
        "email": "stuart@blackpig.eu",
        "role": "Developer"
      }
    ],
    "require": {
      "php": "^8.2|^8.3|^8.4",
      "filament/filament": "^4.0",
      "illuminate/contracts": "^11.0",
      "spatie/laravel-medialibrary": "^11.0"
    },
    "require-dev": {
      "orchestra/testbench": "^9.0",
      "pestphp/pest": "^3.0",
      "pestphp/pest-plugin-laravel": "^3.0",
      "phpunit/phpunit": "^11.0"
    },
    "autoload": {
      "psr-4": {
        "Blackpigcreatif\\Atelier\\": "src/",
        "Blackpigcreatif\\Atelier\\Database\\Factories\\": "database/factories/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Blackpigcreatif\\Atelier\\Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": "vendor/bin/pest",
      "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
      "sort-packages": true,
      "allow-plugins": {
        "pestphp/pest-plugin": true
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Blackpigcreatif\\Atelier\\AtelierServiceProvider"
        ],
        "aliases": {
          "Atelier": "Blackpigcreatif\\Atelier\\Facades\\Atelier"
        }
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }